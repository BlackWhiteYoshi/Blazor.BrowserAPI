@namespace BrowserAPI.Test.Client

<div class="group">
    <h2>HTML Element InProcess</h2>
    <div>
        <button data-testid="@BUTTON_GET_INNERTEXT" @onclick="GetInnerText">GetInnerText</button>
        <button data-testid="@BUTTON_SET_INNERTEXT" @onclick="SetInnerText">SetInnerText</button>
        <button data-testid="@BUTTON_GET_OUTERTEXT" @onclick="GetOuterText">GetOuterText</button>
        <button data-testid="@BUTTON_SET_OUTERTEXT" @onclick="SetOuterText">SetOuterText</button>
        <button data-testid="@BUTTON_GET_STYLE" @onclick="GetStyle">GetStyle</button>
        <button data-testid="@BUTTON_SET_STYLE" @onclick="SetStyle">SetStyle</button>

        <button data-testid="@BUTTON_GET_OFFSETWIDTH" @onclick="GetOffsetWidth">GetOffsetWidth</button>
        <button data-testid="@BUTTON_GET_OFFSETHEIGHT" @onclick="GetOffsetHeight">GetOffsetHeight</button>
        <button data-testid="@BUTTON_GET_OFFSETLEFT" @onclick="GetOffsetLeft">GetOffsetLeft</button>
        <button data-testid="@BUTTON_GET_OFFSETTOP" @onclick="GetOffsetTop">GetOffsetTop</button>
        <button data-testid="@BUTTON_GET_OFFSETPARENT" @onclick="GetOffsetParent">GetOffsetParent</button>

        <button data-testid="@BUTTON_GET_HASFOCUS" @onclick="GetHasFocus" @onpointerenter="GetHasFocus">GetHasFocus</button>

        <button data-testid="@BUTTON_CLICK" @onclick="Click">Click</button>
        <button data-testid="@BUTTON_FOCUS" @onclick="Focus">Focus</button>
        <button data-testid="@BUTTON_BLUR" @onclick="Blur" @onpointerenter="Blur">Blur</button>
        <button data-testid="@BUTTON_SHOW_POPOVER" @onclick="ShowPopover">ShowPopover</button>
        <button data-testid="@BUTTON_HIDE_POPOVER" @onclick="HidePopover">HidePopover</button>
        <button data-testid="@BUTTON_TOGGLE_POPOVER" @onclick="TogglePopover">TogglePopover</button>
        <button data-testid="@BUTTON_TOGGLE_POPOVER_PARAMETER" @onclick="TogglePopoverParameter">TogglePopoverParameter</button>


        <button data-testid="@BUTTON_GET_INNERHTML" @onclick="GetInnerHTML">GetInnerHTML</button>
        <button data-testid="@BUTTON_SET_INNERHTML" @onclick="SetInnerHTML">SetInnerHTML</button>
        <button data-testid="@BUTTON_GET_OUTERHTML" @onclick="GetOuterHTML">GetOuterHTML</button>
        <button data-testid="@BUTTON_SET_OUTERHTML" @onclick="SetOuterHTML">SetOuterHTML</button>
        <button data-testid="@BUTTON_GET_ATTRIBUTES" @onclick="GetAttributes">GetAttributes</button>
        <button data-testid="@BUTTON_GET_CHILD_ELEMENT_COUNT" @onclick="GetChildElementCount">GetChildElementCount</button>
        <button data-testid="@BUTTON_GET_CHILDREN" @onclick="GetChildren">GetChildren</button>
        <button data-testid="@BUTTON_GET_CLASSNAME" @onclick="GetClassName">GetClassName</button>
        <button data-testid="@BUTTON_SET_CLASSNAME" @onclick="SetClassName">SetClassName</button>
        <button data-testid="@BUTTON_GET_CLASSLIST" @onclick="GetClassList">GetClassList</button>

        <button data-testid="@BUTTON_GET_CLIENTWIDTH" @onclick="GetClientWidth">GetClientWidth</button>
        <button data-testid="@BUTTON_GET_CLIENTHEIGHT" @onclick="GetClientHeight">GetClientHeight</button>
        <button data-testid="@BUTTON_GET_CLIENTLEFT" @onclick="GetClientLeft">GetClientLeft</button>
        <button data-testid="@BUTTON_GET_CLIENTTOP" @onclick="GetClientTop">GetClientTop</button>

        <button data-testid="@BUTTON_GET_SCROLLWIDTH" @onclick="GetScrollWidth">GetScrollWidth</button>
        <button data-testid="@BUTTON_GET_SCROLLHEIGHT" @onclick="GetScrollHeight">GetScrollHeight</button>
        <button data-testid="@BUTTON_GET_SCROLLLEFT" @onclick="GetScrollLeft">GetScrollLeft</button>
        <button data-testid="@BUTTON_SET_SCROLLLEFT" @onclick="SetScrollLeft">SetScrollLeft</button>
        <button data-testid="@BUTTON_GET_SCROLLTOP" @onclick="GetScrollTop">GetScrollTop</button>
        <button data-testid="@BUTTON_SET_SCROLLTOP" @onclick="SetScrollTop">SetScrollTop</button>

        <button data-testid="@BUTTON_GET_BOUNDING_CLIENT_RECT" @onclick="GetBoundingClientRect">GetBoundingClientRect</button>
        <button data-testid="@BUTTON_GET_CLIENT_RECTS" @onclick="GetClientRects">GetClientRects</button>
        <button data-testid="@BUTTON_HAS_ATTRIBUTE" @onclick="HasAttribute">HasAttribute</button>
        <button data-testid="@BUTTON_HAS_ATTRIBUTES" @onclick="HasAttributes">HasAttributes</button>
        <button data-testid="@BUTTON_SCROLL" @onclick="Scroll">Scroll</button>
        <button data-testid="@BUTTON_SCROLL_BY" @onclick="ScrollBy">ScrollBy</button>
        <button data-testid="@BUTTON_SCROLL_INTO_VIEW" @onclick="ScrollIntoView">ScrollIntoView</button>
        <button data-testid="@BUTTON_REQUEST_FULLSCREEN" @onclick="RequestFullscreen">RequestFullscreen</button>


        <button data-testid="@BUTTON_REGISTER_ON_TRANSITIONSTART" @onclick="RegisterOnTransitionstart">RegisterOnTransitionstart</button>
        <button data-testid="@BUTTON_REGISTER_ON_TRANSITIONEND" @onclick="RegisterOnTransitionend">RegisterOnTransitionend</button>
        <button data-testid="@BUTTON_REGISTER_ON_TRANSITIONRUN" @onclick="RegisterOnTransitionrun">RegisterOnTransitionrun</button>
        <button data-testid="@BUTTON_REGISTER_ON_TRANSITIONCANCEL" @onclick="RegisterOnTransitioncancel">RegisterOnTransitioncancel</button>
        <button data-testid="@BUTTON_REGISTER_ON_ANIMATIONSTART" @onclick="RegisterOnAnimationstart">RegisterOnAnimationstart</button>
        <button data-testid="@BUTTON_REGISTER_ON_ANIMATIONEND" @onclick="RegisterOnAnimationend">RegisterOnAnimationend</button>
        <button data-testid="@BUTTON_REGISTER_ON_ANIMATIONITERATION" @onclick="RegisterOnAnimationiteration">RegisterOnAnimationiteration</button>
        <button data-testid="@BUTTON_REGISTER_ON_ANIMATIONCANCEL" @onclick="RegisterOnAnimationcancel">RegisterOnAnimationcancel</button>
    </div>
    <label data-testid="@LABEL_OUTPUT">@labelOutput</label>
    <div data-testid="@HTML_ELEMENT_CONTAINER" class="html-element-container">
        <div data-testid="@HTML_ELEMENT" @ref="htmlElement" class="html-element" tabindex="-1" style="visibility: visible;"
                @onpointerdown="SetPointerCapture" @onpointermove="HasPointerCapture" @onpointerup="ReleasePointerCapture">
            test <b>bold</b> <i>italic</i> <u>underlined</u>
        </div>
    </div>
    <p data-testid="@POPOVER_ELEMENT" @ref="popoverElement" class="html-element-popover" popover>Popover</p>
</div>
