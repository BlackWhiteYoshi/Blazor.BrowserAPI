        <button data-testid="@BUTTON_REGISTER_ON_DEVICE_MOTION" @onclick="RegisterOnDeviceMotion">RegisterOnDeviceMotion</button>
        <button data-testid="@BUTTON_REGISTER_ON_DEVICE_ORIENTATION" @onclick="RegisterOnDeviceOrientation">RegisterOnDeviceOrientation</button>
        <button data-testid="@BUTTON_REGISTER_ON_DEVICE_ORIENTATION_ABSOLUTE" @onclick="RegisterOnDeviceOrientationAbsolute">RegisterOnDeviceOrientationAbsolute</button>
