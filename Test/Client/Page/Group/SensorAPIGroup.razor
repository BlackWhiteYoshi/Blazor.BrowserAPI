@namespace BrowserAPI.Test.Client

<div class="group">
    <h2>Sensor API</h2>
    <div>
        @* Sensor Properties *@
        <button data-testid="@BUTTON_GET_ACTIVATED_PROPERTY" @onclick="GetActivated_Property">GetActivated_Property</button>
        <button data-testid="@BUTTON_GET_ACTIVATED_METHOD" @onclick="GetActivated_Method">GetActivated_Method</button>
        <button data-testid="@BUTTON_GET_HAS_READING_PROPERTY" @onclick="GetHasReading_Property">HasReading_Property</button>
        <button data-testid="@BUTTON_GET_HAS_READING_METHOD" @onclick="GetHasReading_Method">HasReading_Method</button>
        <button data-testid="@BUTTON_GET_TIMESTAMP_PROPERTY" @onclick="GetTimestamp_Property">GetTimestamp_Property</button>
        <button data-testid="@BUTTON_GET_TIMESTAMP_METHOD" @onclick="GetTimestamp_Method">GetTimestamp_Method</button>
        @* Sensor methods *@
        <button data-testid="@BUTTON_START" @onclick="Start">Start</button>
        <button data-testid="@BUTTON_STOP" @onclick="Stop">Stop</button>
        @* Sensor Events *@
        <button data-testid="@BUTTON_REGISTER_ON_ERROR" @onclick="RegisterOnError">RegisterOnError</button>
        <button data-testid="@BUTTON_REGISTER_ON_ACTIVATE" @onclick="RegisterOnActivate">RegisterOnActivate</button>
        <button data-testid="@BUTTON_REGISTER_ON_READING" @onclick="RegisterOnReading">RegisterOnReading</button>
